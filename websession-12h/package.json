{
  "name": "websession-12h",
  "author": "Nat",
  "version": "0.0.4",
  "description": "open the AWS web console in a session that can last up to 12h",
  "keywords": [
    "leapp-plugin",
    "web sessions",
    "AWS"
  ],
  "leappPlugin": {
    "supportedOS": [
      "mac",
      "windows",
      "linux"
    ],
    "supportedSessions": [
      "awsIamRoleFederated",
      "awsIamRoleChained",
      "awsSsoRole"
    ]
  },
  "scripts": {
    "build": "webpack --config webpack.config.js"
  },
  "files": [
    "plugin.js"
  ],
  "license": "ISC",
  "dependencies": {
    "@noovolari/leapp-core": "^0.1.117"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "path-browserify": "^1.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
